{
  "name": "client-tool",
  "version": "0.0.1",
  "description": "client-tool",
  "main": "out_client/main.js",
  "scripts": {
    "dev:client": "yarn check:src && cd build && ttsc -p tsconfig.build.json && cd .. && node --max_old_space_size=4095 ./node_modules/gulp/bin/gulp.js dev-client && electron .",
    "check:src": "cd src && ttsc -p tsconfig.json",
    "build:client32": "yarn check:src && electron-builder --win --ia32",
    "build:client64": "yarn check:src && electron-builder",
    "build:client": " cd build && ttsc -p tsconfig.build.json && cd .. && node --max_old_space_size=4095 ./node_modules/gulp/bin/gulp.js build-client && electron-builder --win --ia32",
    "package-client": "cd src && ttsc -p tsconfig.json && cd .. && electron-builder --win --x64",
    "test:client": "electron .",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "dev:electron": "cd packages/app-electron && cross-env APP_ENV=dev webpack &&  cd ../..dist/app-electron && electron .",
    "qa:extension": "cd packages/app-extension && cross-env NODE_ENV=qa webpack --watch",
    "pre:extension": "cd packages/app-extension && cross-env NODE_ENV=pre webpack --watch",
    "prod:extension": "cd packages/app-extension && cross-env NODE_ENV=prod webpack --watch",
    "dev:biz": "cd packages/app-biz && cross-env NODE_ENV=dev webpack --watch",
    "qa:biz": "cd packages/app-biz && cross-env NODE_ENV=qa webpack --watch",
    "pre:biz": "cd packages/app-biz && cross-env NODE_ENV=pre webpack --watch",
    "prod:biz": "cd packages/app-biz && cross-env NODE_ENV=prod webpack --watch",
    "dev:client-old": "cd packages/app-client && cross-env NODE_ENV=production webpack --watch"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "productName": "client-tool",
    "appId": "com.code.client-tool",
    "directories": {
      "output": "dist"
    },
    "extraResources": {
      "from": "./public/",
      "to": "public"
    },
    "files": [
      "out_client/**/*",
      "out_page/**/*",
      "product.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "public/static/app.ico"
    },
    "win": {
      "icon": "public/static/app.ico",
      "requestedExecutionLevel": "highestAvailable"
    },
    "linux": {
      "icon": "public/static/app.ico"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "^0.0.135",
    "@types/debounce": "^1.2.0",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp-filter": "^3.0.33",
    "@types/gulp-rename": "^2.0.0",
    "@types/ini": "^1.3.30",
    "@types/node": "^14.14.37",
    "@types/rimraf": "^3.0.0",
    "@types/through": "^0.0.30",
    "@types/underscore": "^1.11.2",
    "@types/winreg": "^1.2.30",
    "electron": "^12.0.9",
    "electron-builder": "^22.11.7",
    "gulp": "^4.0.2",
    "gulp-filter": "^7.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "rimraf": "^3.0.2",
    "source-map": "0.6.1",
    "through": "^2.3.8",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.0-dev.20210708",
    "typescript-transform-paths": "^2.2.4",
    "underscore": "^1.13.1",
    "vinyl": "^2.2.1"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "axios": "^0.21.1",
    "debounce": "^1.2.1",
    "electron-pdf-window": "^1.0.12",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "fancy-log": "^1.3.3",
    "find-process": "^1.4.4",
    "fs-extra": "^10.0.0",
    "fs-iconv": "^3.1.5",
    "gulp-tsb": "^4.0.6",
    "iconv-lite": "^0.6.3",
    "ini": "^2.0.0",
    "log4js": "^6.3.0",
    "node-localstorage": "^2.1.6",
    "node-netstat": "^1.8.0",
    "node-uuid": "^1.4.8",
    "readline": "^1.3.0",
    "regedit": "^3.0.3",
    "sqlite3": "^5.0.2"
  }
}
