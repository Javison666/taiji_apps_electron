{
	"name": "client-tool",
	"version": "1.0.1",
	"description": "client-tool",
	"main": "out_client/main.js",
	"scripts": {
		"init:build": "cp build/package.json package.json",
		"init:pre": "cp build_pre/package.json package.json",
		"check:src": "cd src && ttsc -p tsconfig.json",
		"dev:client": "yarn check:src && cd build && ttsc -p tsconfig.build.json && cd .. && node --max_old_space_size=4095 ./node_modules/gulp/bin/gulp.js dev-client && electron . --env-dev",
		"prod:client": "yarn check:src && cd build && ttsc -p tsconfig.build.json && cd .. && node --max_old_space_size=4095 ./node_modules/gulp/bin/gulp.js dev-client && electron .",
		"build:static": "cd build && ttsc -p tsconfig.build.json && cd .. && node --max_old_space_size=4095 ./node_modules/gulp/bin/gulp.js build-client",
		"build:client": "yarn build:static && ./dist_pre/win-ia32-unpacked/client-prebuild.exe && electron-builder --win --ia32 -c ./build/electron-builder.json",
		"build:client64": "yarn build:static && ./dist_pre/win-unpacked/client-prebuild.exe && electron-builder --win -c ./build/electron-builder.json"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/chrome": "^0.0.135",
		"@types/debounce": "^1.2.0",
		"@types/electron": "^1.6.10",
		"@types/express": "^4.17.13",
		"@types/express-ws": "^3.0.1",
		"@types/fancy-log": "^1.3.1",
		"@types/gulp-filter": "^3.0.33",
		"@types/gulp-rename": "^2.0.0",
		"@types/ini": "^1.3.30",
		"@types/node": "^14.14.37",
		"@types/rimraf": "^3.0.0",
		"@types/through": "^0.0.30",
		"@types/underscore": "^1.11.2",
		"@types/winreg": "^1.2.30",
		"electron": "^12.0.9",
		"electron-builder": "^22.11.7",
		"gulp": "^4.0.2",
		"gulp-filter": "^7.0.0",
		"gulp-rename": "^2.0.0",
		"gulp-uglify": "^3.0.2",
		"rimraf": "^3.0.2",
		"source-map": "0.6.1",
		"through": "^2.3.8",
		"ttypescript": "^1.5.12",
		"typescript": "^4.4.0-dev.20210708",
		"typescript-transform-paths": "^2.2.4",
		"underscore": "^1.13.1",
		"vinyl": "^2.2.1"
	},
