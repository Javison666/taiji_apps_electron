		"bytenode": "^1.3.4",
		"cheerio": "^1.0.0-rc.10",
		"debounce": "^1.2.1",
		"event-stream": "^4.0.1",
		"express": "^4.17.1",
		"express-ws": "^5.0.2",
		"fancy-log": "^1.3.3",
		"ffi-napi": "^4.0.3",
		"find-process": "^1.4.4",
		"fs-extra": "^10.0.0",
		"fs-iconv": "^3.1.5",
		"got": "^11.8.3",
